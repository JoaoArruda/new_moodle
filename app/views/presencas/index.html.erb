<p id="notice"><%= notice %></p>

<h1>Presencas</h1>

<table>
  <thead>
    <tr>
      <th>Disciplina</th>
      <th>Belongs to</th>
      <th>Aluno</th>
      <th>Belongs to</th>
      <th>Data</th>
      <th>Curso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @presencas.each do |presenca| %>
      <tr>
        <td><%= presenca.disciplina %></td>
        <td><%= presenca.belongs_to %></td>
        <td><%= presenca.aluno %></td>
        <td><%= presenca.belongs_to %></td>
        <td><%= presenca.data %></td>
        <td><%= presenca.curso %></td>
        <td><%= link_to 'Show', presenca %></td>
        <td><%= link_to 'Edit', edit_presenca_path(presenca) %></td>
        <td><%= link_to 'Destroy', presenca, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Presenca', new_presenca_path %>
